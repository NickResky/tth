{
  "name": "tth-client",
  "version": "0.0.0",
  "license": "MIT",
  "engines": {
    "node": "8.11.3"
  },
  "scripts": {
    "start-local": "ng serve",
    "ng": "./node_modules/.bin/ng",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "serve:ssr": "ts-node ./src/server",
    "build:client-and-server-bundles": "node ./node_modules/.bin/ng build && node ./node_modules/.bin/ng run app:server",
    "webpack:server": "webpack --config ./webpack.server.config.js --progress --colors",
    "install-dev": "npm install --only=development",
    "start": "npm run serve:ssr",
    "lint": "npm run ng lint",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
    "test": "karma start ./karma.conf.js",
    "e2e": "protractor ./protractor.conf.js"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/animations": "^7.2.16",
    "@angular/common": "^7.2.16",
    "@angular/core": "^7.2.16",
    "@angular/forms": "^7.2.16",
    "@angular/http": "^7.2.16",
    "@angular/platform-browser": "^7.2.16",
    "@angular/platform-browser-dynamic": "^7.2.16",
    "@angular/platform-server": "^7.2.16",
    "@angular/router": "^7.2.16",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "^2.0.10",
    "@types/lodash": "^4.14.179",
    "@types/node": "^6.14.13",
    "@vimeo/player": "^2.16.4",
    "bowser": "^2.11.0",
    "core-js": "^2.6.12",
    "hoek": "^5.0.4",
    "lodash": "^4.17.21",
    "protractor": "^5.4.4",
    "rxjs": "^6.6.7",
    "rxjs-compat": "^6.6.7",
    "ts-loader": "^4.5.0",
    "ts-node": "~3.2.0",
    "tslib": "^1.9.0",
    "typescript": "3.2.4",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular/cli": "^7.3.10",
